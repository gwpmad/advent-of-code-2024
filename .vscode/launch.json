{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Run Specific Day",
      "type": "debugpy",
      "request": "launch",
      "program": "run_day.py",
      "args": [
        "${input:dayNumber}"
      ],
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ],
  "inputs": [
    {
      "type": "promptString",
      "id": "dayNumber",
      "description": "Day number?",
      "default": "1"
    }
  ]
}